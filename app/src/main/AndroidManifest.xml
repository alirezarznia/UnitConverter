<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.unitconvertor">

    <application
        android:allowBackup="true"
        android:name="com.example.unitconverter.G"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.unitconverter.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.unitconverter.DistanceActivity">
        </activity>
        <activity android:name="com.example.unitconverter.EnhancedActivity">
        </activity>
        <activity android:name="com.example.unitconverter.VolumeActivity">
        </activity>
        <activity android:name="com.example.unitconverter.SpeedActivity"></activity>
        <activity android:name="com.example.unitconverter.TemperatureActivity"></activity>
    </application>

</manifest>